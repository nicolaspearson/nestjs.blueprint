{
  "name": "dellingr",
  "version": "0.0.1",
  "description": "This is NestJS starter repository.",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/nicolaspearson/nestjs.dellingr.git"
  },
  "author": "Nicolas Pearson",
  "license": "MIT",
  "engines": {
    "node": ">=v16.13.2",
    "npm": "please-use-yarn",
    "yarn": ">=v3.1.1"
  },
  "engineStrict": true,
  "main": "src/index.ts",
  "scripts": {
    "build": "rimraf dist && webpack --config webpack.config.js",
    "build:tsc": "rimraf dist && tsc --project tsconfig.tsc.json",
    "db:logs": "yarn exec:docker-compose logs -f db",
    "db:migration:create": "typeorm migration:create -n",
    "db:migration:generate:missing": "yarn exec:script:generate-migration public",
    "db:migration:generate": "typeorm migration:generate -n",
    "db:migration:revert": "typeorm migration:revert",
    "db:migration:run": "yarn build:tsc && typeorm migration:run",
    "db:start": "yarn exec:docker-compose up -d --renew-anon-volumes --force-recreate db",
    "db:stop": "docker-compose rm -f -s -v db",
    "docker:build": "yarn exec:docker-compose build --no-cache dellingr",
    "docker:logs": "yarn exec:docker-compose logs -f dellingr",
    "docker:start": "yarn exec:docker-compose up -d --build dellingr",
    "doctor": "yarn dlx @yarnpkg/doctor .",
    "exec:docker-compose": "docker-compose -f docker-compose.yaml",
    "exec:eslint": "eslint \"{src,test}/**/*.ts\" --cache",
    "exec:jest": "jest --coverage --color --forceExit --runInBand",
    "exec:prettier": "prettier",
    "exec:script:generate-migration": "./scripts/shell/generate-migration.sh",
    "lint:eslint": "yarn exec:eslint --cache-file .eslintcache .",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "lint:markdown": "markdownlint -c markdownlint.yml $(find . -name '*.md' -not -path './.yarn/*')",
    "lint:prettier": "yarn exec:prettier --config .prettierrc --ignore-path .prettierignore --no-editorconfig --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint:prettier:fix": "yarn exec:prettier --config .prettierrc --ignore-path .prettierignore --no-editorconfig --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "yarn lint:eslint && yarn lint:markdown && yarn lint:prettier",
    "lint:fix": "yarn lint:eslint:fix && yarn lint:prettier:fix",
    "postinstall": "husky install",
    "sdk": "yarn dlx @yarnpkg/sdks vscode",
    "start:dev": "rimraf dist && yarn db:start && nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start": "node dist/server.js",
    "test:integration:ci": "yarn exec:jest --config test/integration/jest.config.js",
    "test:integration": "yarn db:start && yarn test:integration:ci && yarn db:stop",
    "test:unit": "yarn exec:jest --config test/unit/jest.config.js"
  },
  "dependencies": {
    "$": "link:./src",
    "@nestjs/common": "^8.2.6",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.2.6",
    "@nestjs/platform-express": "^8.2.6",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/typeorm": "^8.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "common-tags": "^1.8.2",
    "dotenv": "^14.3.0",
    "express": "^4.17.2",
    "helmet": "^5.0.2",
    "joi": "^17.5.0",
    "nocache": "^3.0.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.2",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "#": "link:./test",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@nestjs/cli": "^8.2.0",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.6",
    "@types/common-tags": "^1.8.1",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.0",
    "@types/joi": "^17.2.3",
    "@types/node": "^16.11.21",
    "@types/pg": "^8.6.4",
    "@types/supertest": "^2.0.11",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-ordered-imports": "^0.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-unicorn": "^40.1.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "markdownlint-cli": "^0.30.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "run-script-webpack-plugin": "^0.0.11",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.2",
    "webpack-pnp-externals": "^1.1.0"
  },
  "packageManager": "yarn@3.1.1"
}
