{
  "name": "nestjs.blueprint",
  "version": "0.0.1",
  "description": "This is NestJS starter repository.",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/nicolaspearson/veritas.git"
  },
  "author": "Nicolas Pearson",
  "license": "MIT",
  "engines": {
    "node": ">=v16.13.2"
  },
  "main": "src/index.ts",
  "scripts": {
    "build": "rimraf dist && nest build",
    "doctor": "yarn dlx @yarnpkg/doctor .",
    "exec:docker-compose": "docker-compose -f docker-compose.yaml",
    "exec:eslint": "eslint \"{src,test}/**/*.ts\" --cache",
    "exec:jest": "jest --coverage --color --forceExit --runInBand",
    "exec:prettier": "prettier",
    "lint:eslint": "yarn exec:eslint --cache-file $(pwd)/.eslintcache $(pwd)",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "lint:markdown": "markdownlint -c markdownlint.yml $(find . -name '*.md' -not -path './.yarn/*')",
    "lint:prettier": "yarn exec:prettier --config .prettierrc --ignore-path .prettierignore --no-editorconfig --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint:prettier:fix": "yarn exec:prettier --config .prettierrc --ignore-path .prettierignore --no-editorconfig --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "yarn lint:eslint && yarn lint:markdown && yarn lint:prettier",
    "lint:fix": "yarn lint:eslint:fix && yarn lint:prettier:fix",
    "postinstall": "husky install",
    "sdk": "yarn dlx @yarnpkg/sdks vscode",
    "start:dev": "nest start --watch",
    "start": "node dist/main",
    "test:integration": "yarn exec:jest --config $(pwd)/test/integration/jest.config.js",
    "test:unit": "yarn exec:jest --config $(pwd)/test/unit/jest.config.js"
  },
  "dependencies": {
    "$": "link:./src",
    "@nestjs/common": "^8.2.6",
    "@nestjs/core": "^8.2.6",
    "@nestjs/platform-express": "^8.2.6",
    "express": "^4.17.2",
    "nocache": "^3.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.2"
  },
  "devDependencies": {
    "#": "link:./test",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@nestjs/cli": "^8.2.0",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.6",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.0",
    "@types/node": "^16.11.21",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-ordered-imports": "^0.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-unicorn": "^40.1.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "markdownlint-cli": "^0.30.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5",
    "webpack": "^5.67.0"
  },
  "packageManager": "yarn@3.1.1"
}
